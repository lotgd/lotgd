<div {{ attributes.defaults({}) }}>
    {{ form_start(form, {
        "attr": {
            'data-action': 'live#action:prevent',
            'data-live-action-param': 'save'
        }
    }) }}
        {{ form_rest(form) }}
        <button class="btn btn-primary">Submit</button>
    {{ form_end(form) }}
</div>